const subjects = ["BM","BI","MM","SAINS","PAI","PMZ","PSV","PJPK","AR"];
const colors = ["#FF6384","#36A2EB","#FFCE56","#4BC0C0","#9966FF","#FF9F40","#8BC34A","#E91E63","#00BCD4"];

const kelasSelect = document.getElementById('kelasSelect');
const muridSelect = document.getElementById('muridSelect');
const tbody = document.querySelector('#tbl tbody');

// populate kelas dropdown
const kelasList = [...new Set(dataMurid.map(d=>d.kelas))].sort();
kelasList.forEach(k=>{
  const opt = document.createElement('option'); opt.value = k; opt.text = k; kelasSelect.appendChild(opt);
});

// render table (filter by kelas)
function renderTable(kelas='ALL'){
  tbody.innerHTML = '';
  const rows = dataMurid.filter(d => kelas === 'ALL' ? true : d.kelas === kelas);
  rows.forEach(r=>{
    const tr = document.createElement('tr');
    tr.innerHTML = `<td>${r.nama}</td><td>${r.kelas}</td>` + subjects.map(s=>`<td>${r[s]}</td>`).join('');
    tbody.appendChild(tr);
  });
}

// populate murid dropdown for selected kelas
function populateMurid(kelas='ALL'){
  muridSelect.innerHTML = '<option value="">--Pilih Murid--</option>';
  if(kelas === 'ALL') return;
  dataMurid.filter(d=>d.kelas===kelas).forEach(d=>{
    const o = document.createElement('option'); o.value = d.nama; o.text = d.nama; muridSelect.appendChild(o);
  });
}

// create bar chart
const ctx = document.getElementById('barChart');
const barChart = new Chart(ctx, {
  type: 'bar',
  data: { labels: subjects, datasets: [] },
  options: {
    responsive:true,
    scales: { y: { min: 0, max: 6, ticks:{stepSize:1} } },
    plugins: { legend:{ display:false } }
  }
});

function showStudent(name){
  const student = dataMurid.find(s=>s.nama===name);
  if(!student) return;
  const data = subjects.map(s => student[s]);
  barChart.data.datasets = [{
    label: student.nama,
    data,
    backgroundColor: colors,
    borderColor: colors,
    borderWidth: 1
  }];
  barChart.update();
}

// CSV export
function exportCSV(){
  const header = ['NAMA','KELAS',...subjects].join(',');
  const lines = dataMurid.map(d=>[d.nama,d.kelas,...subjects.map(s=>d[s])].map(v=>{
    const t = String(v).replace(/"/g,'""'); return /,|\n|"/.test(t)?`"${t}"`:t;
  }).join(','));
  const csv = [header,...lines].join('\n');
  const blob = new Blob([csv], {type:'text/csv;charset=utf-8'});
  const url = URL.createObjectURL(blob);
  const a = document.createElement('a'); a.href = url; a.download = 'PBD_SK_Sepri_Tengah.csv'; a.click();
  URL.revokeObjectURL(url);
}

// events
kelasSelect.addEventListener('change', ()=>{
  const k = kelasSelect.value;
  renderTable(k);
  populateMurid(k);
  barChart.data.datasets = []; barChart.update();
  muridSelect.value = "";
});

muridSelect.addEventListener('change', ()=> showStudent(muridSelect.value));

document.getElementById('btnCsv').addEventListener('click', exportCSV);

// init
renderTable('ALL');

// header shrink on scroll (logo animate)
const header = document.getElementById('pageHeader');
window.addEventListener('scroll', ()=> header.classList.toggle('shrink', window.scrollY > 40));
</script>
</body>
</html>
